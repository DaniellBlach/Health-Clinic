{% extends 'base.html.twig' %}

{% block title %}Wystaw Receptę{% endblock %}

{% block body %}
    <div class="container w-25">
        <div class="container form" style="margin-top:100px;">
            <h1 class="text-center">Wystaw receptę</h1>
            {{ form_start(form) }}
            {{ form_row(form.expirationDate) }}
            <button type="submit" class="btn btn-green mt-2 mb-4">Wystaw receptę</button>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
